    "source": "https://github.com/MetaModels/attribute_translatedtablemulti"
  },
  "require": {
    "php": ">=5.6 | ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.1",
    "contao/manager-bundle": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedTableMultiBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTranslatedTableMultiBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_translatedtablemulti-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master":  "2.0.x-dev",
      "dev-develop": "2.1.x-dev",
      "dev-feature/contao4": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]

}
