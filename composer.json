{
  "name": "menatwork/attribute_translatedmulti",
  "description": "MetaModels attribute for dynamic translated multi table",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "text",
    "tabletext",
    "translatable",
    "multicolumnwizard"
  ],
  "type": "contao-bundle",
  "homepage": "http://www.men-at-work.de/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    },
    {
      "name": "Andreas Dziemba",
      "email": "dziemba@men-at-work.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/menatwork/attribute_translatedmulti/issues",
    "source": "https://github.com/menatwork/attribute_translatedmulti"
  },
  "require": {
    "php": ">=5.4 | ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.1",
    "contao/manager-plugin": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedMultiBundle\\": "src"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTranslatedMultiBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_translatedmulti-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.1.x-dev",
      "dev-develop": "2.2.x-dev"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]

}
